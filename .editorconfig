root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
quote_type = single

[*.ts]
indent_style = space
indent_size = 2


[*.js]
indent_style = space
indent_size = 2


[*.json]
indent_style = space
indent_size = 2


[*.md]
trim_trailing_whitespace = false
